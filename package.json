{
  "name": "@cn-zhubin-007/branch-compare",
  "version": "1.0.12",
  "description": "Git branch comparison tool",
  "main": "index.js",
  "bin": {
    "@cn-zhubin-007/branch-compare": "./index.js",
    "branch-compare": "./index.js"
  },
  "scripts": {
    "start": "node index.js",
    "pkg:fix": "npm pkg fix",
    "postinstall": "node -e \"const fs=require('fs'); const path=require('path'); const dir=path.join(require('os').homedir(), '.config', 'branch-compare'); !fs.existsSync(dir) && fs.mkdirSync(dir, {recursive:true});\""
  },
  "files": [
    "index.js",
    "lib/",
    "templates/",
    "README.md"
  ],
  "keywords": [
    "git",
    "branch",
    "compare",
    "diff",
    "cli"
  ],
  "author": "cn-zhubin-007",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhubincode/branch-compare.git"
  },
  "bugs": {
    "url": "https://github.com/zhubincode/branch-compare/issues"
  },
  "homepage": "https://github.com/zhubincode/branch-compare#readme",
  "dependencies": {
    "chalk": "^4.1.2",
    "inquirer": "^8.2.5",
    "inquirer-autocomplete-prompt": "^2.0.0"
  },
  "engines": {
    "node": ">=12"
  }
}
